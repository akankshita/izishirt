  def get_currency

	if ! @billingaddress || ! @billingaddress.country
		return "CAD"
	end
	
    return "EUR" if @billingaddress.country.is_europe && @billingaddress.country.shortname != "GB"

    case @billingaddress.country.shortname
      when 'CA' 
        return 'CAD'
      when 'AU' 
        return 'AUD'
      when 'US' 
        return 'USD'
      when 'EU'
        return 'EUR'
      when 'MX'
        return 'MXN'
      when 'ES'
        return 'EUR'
      when 'BR'
        return 'BRL'
      # when 'RE'
      #  return 'EUR'
      # when 'GP'
      #  return 'EUR'
      # when 'MQ'
      #  return 'EUR'
      # when 'GY'
      #  return 'EUR'
      when 'GB'
        return 'GBP'
      when 'FR'
        return 'EUR'
      when 'BE'
        return 'EUR'
      when 'CH'
        return 'CHF'
      when 'IN'
        return 'USD'
      else 
        return 'CAD'
    end
  end

